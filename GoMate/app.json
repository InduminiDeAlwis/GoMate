{
  "expo": {
    "name": "GoMate",
    "slug": "GoMate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gomate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.indumini.GoMate"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0a7ea4",
          "dark": {
            "backgroundColor": "#1a1a2e"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "TRANSPORT_APP_ID": "61c2b84f",
      "TRANSPORT_APP_KEY": "9c5b4fc4dbf37c925cf41bdf214d540e",
      "app_id": "61c2b84f",
      "app_key": "9c5b4fc4dbf37c925cf41bdf214d540e",
      "router": {},
      "eas": {
        "projectId": "6d031468-30d0-4422-8f2b-a97bcaa1ea80"
      }
    }
  }
}
